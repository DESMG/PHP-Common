{
    "name": "desmg/php-common",
    "type": "library",
    "description": "Common Libraries Producted by DESMG",
    "homepage": "https://www.desmg.com",
    "license": "GPL-3.0-only",
    "support": {
        "issues": "https://github.com/DESMG/php-common/issues",
        "source": "https://github.com/DESMG/php-common",
        "group": "https://t.me/desmg_official",
        "telegram": "https://t.me/jyxjjj"
    },
    "authors": [
        {
            "name": "jyxjjj",
            "email": "773933146@qq.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-sockets": "*",
        "ext-ctype": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "autoload": {
        "psr-4": {
            "DESMG\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
